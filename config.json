{
  "additional-repositories": [],
  "archinstall-language": "English",
  "bootloader": "GRUB",
  "hostname": "archpc",
  "kernels": ["linux"],
  "locale_config": {
    "kb_layout": "us",
    "sys_lang": "en_US.UTF-8",
    "sys_enc": "UTF-8"
  },
  "mirror_config": {
    "custom_mirrors": [],
    "mirror_regions": {
      "United States": [
        "https://mirror.arizona.edu/archlinux/$repo/os/$arch"
      ]
    }
  },
  "network_config": {
    "type": "nm"
  },
  "ntp": true,
  "profile_config": {
    "gfx_driver": "All open-source",
    "greeter": "gdm",
    "profile": {
      "custom_settings": { "Gnome": {} },
      "details": [ "Gnome" ],
      "main": "Desktop"
    }
  },
  "script": "guided",
  "swap": true,
  "timezone": "America/Chicago"
}
